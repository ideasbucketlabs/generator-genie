{
    "name": "generator-genie",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "run-p type-check \"build-only {@}\" --",
        "preview": "vite preview",
        "test:unit": "vitest",
        "test:unit:ci": "vitest run --root src/",
        "test:e2e": "playwright test",
        "build-only": "vite build",
        "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
        "lint": "eslint . --fix",
        "format": "prettier --write src/",
        "update-timestamp": "node ./src/scripts/UpdateTimestamp.js",
        "update-css": "node ./src/scripts/UpdateCssFor404.js",
        "process-template": "node ./src/scripts/Processor.js",
        "process-template:watch": "nodemon",
        "prepare": "husky install"
    },
    "dependencies": {
        "@vueuse/core": "^11.1.0",
        "file-saver": "^2.0.5",
        "jszip": "^3.10.1",
        "liquidjs": "^10.8.3",
        "lodash.partition": "^4.6.0",
        "marked": "^14.1.2",
        "pinia": "^2.1.7",
        "prismjs": "^1.29.0",
        "vue": "^3.4.21"
    },
    "devDependencies": {
        "@playwright/test": "^1.48.1",
        "@tsconfig/node20": "^20.1.4",
        "@types/jsdom": "^21.1.7",
        "@types/node": "^20.17.0",
        "@vitejs/plugin-vue": "^5.1.4",
        "@vitest/eslint-plugin": "1.1.7",
        "@vue/eslint-config-prettier": "^10.0.0",
        "@vue/eslint-config-typescript": "^14.1.1",
        "@vue/test-utils": "^2.4.6",
        "@vue/tsconfig": "^0.5.1",
        "eslint": "^9.13.0",
        "eslint-plugin-playwright": "^2.0.0",
        "eslint-plugin-vue": "^9.29.0",
        "jsdom": "^25.0.1",
        "npm-run-all2": "^7.0.1",
        "prettier": "^3.3.3",
        "typescript": "~5.6.0",
        "vite": "^5.4.10",
        "vitest": "^2.1.3",
        "vue-tsc": "^2.1.6",
        "@tailwindcss/forms": "^0.5.3",
        "@tailwindcss/typography": "^0.5.9",
        "@types/file-saver": "^2.0.5",
        "@types/lodash.partition": "^4.6.7",
        "@types/prismjs": "^1.26.1",
        "@vitest/coverage-v8": "^2.1.2",
        "autoprefixer": "^10.4.14",
        "glob": "^11.0.0",
        "husky": "^9.0.11",
        "nodemon": "^3.0.1",
        "postcss": "^8.4.23",
        "prettier-plugin-tailwindcss": "^0.6.8",
        "tailwindcss": "^3.4.0",
        "ts-node": "^10.9.2"
    }
}
