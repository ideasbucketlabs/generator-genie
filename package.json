{
    "name": "generator-genie",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "dev": "vite",
        "build": "run-p type-check build-only",
        "preview": "vite preview",
        "test:unit": "vitest",
        "test:unit:ci": "vitest run --root src/",
        "test:e2e": "playwright test --workers=3",
        "build-only": "vite build",
        "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
        "format": "prettier --write src/",
        "update-timestamp": "ts-node ./src/util/UpdateTimestamp.ts",
        "process-template": "ts-node ./src/generator/Processor.ts",
        "process-template:watch": "nodemon",
        "prepare": "husky install"
    },
    "dependencies": {
        "@vueuse/core": "^10.1.2",
        "file-saver": "^2.0.5",
        "jszip": "^3.10.1",
        "liquidjs": "^10.8.3",
        "lodash.partition": "^4.6.0",
        "marked": "^9.0.3",
        "pinia": "^2.1.4",
        "prismjs": "^1.29.0",
        "vue": "^3.3.4"
    },
    "devDependencies": {
        "@playwright/test": "^1.36.1",
        "@rushstack/eslint-patch": "^1.3.2",
        "@tailwindcss/forms": "^0.5.3",
        "@tailwindcss/typography": "^0.5.9",
        "@tsconfig/node18": "^18.2.0",
        "@types/file-saver": "^2.0.5",
        "@types/jsdom": "^21.1.1",
        "@types/lodash.partition": "^4.6.7",
        "@types/marked": "^5.0.1",
        "@types/node": "^18.17.0",
        "@types/prismjs": "^1.26.1",
        "@vitejs/plugin-vue": "^4.2.3",
        "@vue/eslint-config-prettier": "^8.0.0",
        "@vue/eslint-config-typescript": "^12.0.0",
        "@vue/test-utils": "^2.4.1",
        "@vue/tsconfig": "^0.4.0",
        "autoprefixer": "^10.4.14",
        "eslint": "^8.45.0",
        "eslint-plugin-vue": "^9.15.1",
        "glob": "^10.3.3",
        "husky": "^8.0.3",
        "jsdom": "^22.1.0",
        "nodemon": "^3.0.1",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.23",
        "prettier": "^3.0.0",
        "prettier-plugin-tailwindcss": "^0.5.3",
        "tailwindcss": "^3.3.2",
        "ts-node": "^10.9.1",
        "typescript": "~5.2.0",
        "vite": "^4.4.6",
        "vitest": "^0.34.1",
        "vue-tsc": "^1.8.6"
    }
}
