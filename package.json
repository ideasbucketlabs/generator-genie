{
    "name": "generator-genie",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "run-p type-check \"build-only {@}\" --",
        "preview": "vite preview",
        "test:unit": "vitest",
        "test:unit:ci": "vitest run --root src/",
        "test:e2e": "playwright test",
        "build-only": "vite build",
        "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
        "format": "prettier --write src/",
        "update-timestamp": "node ./src/scripts/UpdateTimestamp.js",
        "update-css": "node ./src/scripts/UpdateCssFor404.js",
        "process-template": "node ./src/scripts/Processor.js",
        "process-template:watch": "nodemon",
        "prepare": "husky install"
    },
    "dependencies": {
        "@vueuse/core": "^10.1.2",
        "file-saver": "^2.0.5",
        "jszip": "^3.10.1",
        "liquidjs": "^10.8.3",
        "lodash.partition": "^4.6.0",
        "marked": "^12.0.1",
        "pinia": "^2.1.7",
        "prismjs": "^1.29.0",
        "vue": "^3.4.21"
    },
    "devDependencies": {
        "@playwright/test": "^1.43.0",
        "@rushstack/eslint-patch": "^1.8.0",
        "@tailwindcss/forms": "^0.5.3",
        "@tailwindcss/typography": "^0.5.9",
        "@tsconfig/node20": "^20.1.4",
        "@types/file-saver": "^2.0.5",
        "@types/jsdom": "^21.1.6",
        "@types/lodash.partition": "^4.6.7",
        "@types/node": "^20.12.5",
        "@types/prismjs": "^1.26.1",
        "@vitejs/plugin-vue": "^5.0.4",
        "@vitest/coverage-v8": "^1.0.4",
        "@vue/eslint-config-prettier": "^9.0.0",
        "@vue/eslint-config-typescript": "^13.0.0",
        "@vue/test-utils": "^2.4.5",
        "@vue/tsconfig": "^0.5.1",
        "autoprefixer": "^10.4.14",
        "eslint": "^8.57.0",
        "eslint-plugin-playwright": "^1.5.4",
        "eslint-plugin-vue": "^9.23.0",
        "glob": "^10.3.3",
        "husky": "^9.0.11",
        "jsdom": "^24.0.0",
        "nodemon": "^3.0.1",
        "npm-run-all2": "^6.1.2",
        "postcss": "^8.4.23",
        "prettier": "^3.2.5",
        "prettier-plugin-tailwindcss": "^0.5.3",
        "tailwindcss": "^3.4.0",
        "ts-node": "^10.9.2",
        "typescript": "~5.4.0",
        "vite": "^5.2.8",
        "vitest": "^1.4.0",
        "vue-tsc": "^2.0.11"
    }
}
